    "jsonwebtoken": "7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "4.9.5",
    "mongoose-paginate": "^5.0.3",
    "nodemon": "1.11.0",
    "passport": "0.3.2",
    "passport-jwt": "2.2.1",
    "socket.io": "^2.0.3",
    "validator": "^7.0.0"
  },
  "author": "Nizar Hmain",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0"
  }
