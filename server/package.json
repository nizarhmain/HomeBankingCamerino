{
  "name": "example-node-server",
  "version": "1.0.0",
  "description": "Example Node Server w/ Babel",
  "main": "lib/api.js",
  "scripts": {
    "start": "nodemon lib/api.js --exec babel-node --presets es2015,stage-2",
